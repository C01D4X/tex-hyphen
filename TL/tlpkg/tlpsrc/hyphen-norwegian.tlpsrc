category TLCore
depend hyphen-base
depend hyph-utf8
execute AddHyphen \
	name=bokmal synonyms=norwegian,norsk \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-nb.tex \
	file_patterns=hyph-nb.pat.txt \
	file_exceptions=
execute AddHyphen \
	name=nynorsk \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-nn.tex \
	file_patterns=hyph-nn.pat.txt \
	file_exceptions=

