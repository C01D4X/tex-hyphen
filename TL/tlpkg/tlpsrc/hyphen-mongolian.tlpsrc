category TLCore
depend hyphen-base
depend hyph-utf8
execute AddHyphen \
	name=mongolian \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-mn-cyrl.tex \
	file_patterns=hyph-mn-cyrl.pat.txt \
	file_exceptions=hyph-mn-cyrl.hyp.txt
execute AddHyphen \
	name=mongolianlmc \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	luaspecial="disabled:only for 8bit montex with lmc encoding"

