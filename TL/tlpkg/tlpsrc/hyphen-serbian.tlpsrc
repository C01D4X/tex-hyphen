category TLCore
depend hyphen-base
depend hyph-utf8
execute AddHyphen \
	name=serbian \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-sr-latn.tex \
	file_patterns=hyph-sr-latn.pat.txt \
	file_exceptions=hyph-sr-latn.hyp.txt
execute AddHyphen \
	name=serbianc \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-sr-cyrl.tex \
	file_patterns=hyph-sr-cyrl.pat.txt \
	file_exceptions=hyph-sr-cyrl.hyp.txt

