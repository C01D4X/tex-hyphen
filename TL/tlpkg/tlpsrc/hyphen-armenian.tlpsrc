category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Armenian hyphenation patterns.
longdesc Hyphenation patterns for Armenian for Unicode engines.
execute AddHyphen \
	name=armenian \
	lefthyphenmin=1 \
	righthyphenmin=2 \
	file=loadhyph-hy.tex \
	file_patterns=hyph-hy.pat.txt \
	file_exceptions=
srcpattern d source/generic/hyph-utf8/languages/hy
runpattern f tex/generic/hyph-utf8/loadhyph/loadhyph-hy.tex
runpattern f tex/generic/hyph-utf8/patterns/tex/hyph-hy.tex
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-hy.chr.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-hy.hyp.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-hy.lic.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-hy.pat.txt
