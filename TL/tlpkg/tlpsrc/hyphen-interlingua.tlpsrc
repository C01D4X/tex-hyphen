category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Interlingua
longdesc Hyphenation patterns for Interlingua in ASCII encoding.
execute AddHyphen \
	name=interlingua \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-ia.tex \
	file_patterns=hyph-ia.pat.txt \
	file_exceptions=hyph-ia.hyp.txt

