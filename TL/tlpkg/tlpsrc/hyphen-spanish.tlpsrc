category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Spanish hyphenation patterns.
longdesc Hyphenation patterns for Spanish in T1/EC and UTF-8 encodings.
execute AddHyphen \
	name=spanish synonyms=espanol \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-es.tex \
	file_patterns=hyph-es.pat.txt \
	file_exceptions=
srcpattern d doc/generic/hyph-utf8/es
srcpattern d source/generic/hyph-utf8/languages/es
runpattern f tex/generic/hyph-utf8/loadhyph/loadhyph-es.tex
runpattern f tex/generic/hyph-utf8/patterns/ptex/hyph-es.ec.tex
runpattern f tex/generic/hyph-utf8/patterns/tex/hyph-es.tex
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-es.chr.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-es.hyp.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-es.lic.txt
runpattern f tex/generic/hyph-utf8/patterns/txt/hyph-es.pat.txt
