category TLCore
depend hyphen-base
depend hyph-utf8
shortdesc Spanish Hyphenation Patterns
longdesc Hyphenation patterns for Spanish language in T1/EC and UTF-8 encoding.
execute AddHyphen \
	name=spanish synonyms=espanol \
	lefthyphenmin=2 \
	righthyphenmin=2 \
	file=loadhyph-es.tex \
	file_patterns=hyph-es.pat.txt \
	file_exceptions=

